{
  "command": "visualboyadvance-m",
  "saveDirFlag": "General/BatteryDir",
  "screenshotsDirFlag": "General/ScreenshotDir",
  "saveDirArgType": "=",
  "screenshotsDirArgType": "=",
  "extraFlags": [
    "preferences/pauseWhenInactive=0",
    "-f"
   ],
   "activateCommand": "wmctrl -a \"VisualBoyAdvance\"",
   "config": {
    "file": "/home/james/.config/visualboyadvance-m/vbam.ini",
    "controls": {
      "A": {
        "keys": ["Joypad/1","A"]
      },
      "B": {
        "keys": ["Joypad/1","B"]
      },
      "R": {
        "keys": ["Joypad/1","Speed"]
      },
      "L2": {
        "keys": ["Joypad/1","L"]
      },
      "R2": {
        "keys": ["Joypad/1","R"]
      },
      "Axis Y-": {
        "keys": ["Joypad/1","Up"]
      },
      "Axis Y+": {
        "keys": ["Joypad/1","Down"]
      },
      "Axis X-": {
        "keys": ["Joypad/1","Left"]
      },
      "Axis X+": {
        "keys": ["Joypad/1","Right"]
      },
      "Start": {
        "keys": ["Joypad/1","Start"]
      },
      "Select": {
        "keys": ["Joypad/1","Select"]
      }
    },
    "controlFormat": {
      "axis": "Joy1-Axis$CONTROL",
      "button": "Joy1-Button$CONTROL",
      "key": "$CONTROL"
    }
  }
}
