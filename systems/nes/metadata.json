{
  "command": "fceux",
  "saveDirFlag": "--savedir",
  "screenshotsDirFlag": "--sshotdir",
  "saveDirArgType": " ",
  "screenshotsDirArgType": " ",
  "extraFlags": [],
  "fullScreenButtons": ["alt", "enter"],
  "activateCommand": "wmctrl -a 'FCEUX'",
  "config": {
    "file": "/home/james/.fceux/fceux.cfg",
    "controllers": [
      "SDL.Input.GamePad.0",
      "SDL.Input.GamePad.1",
      "SDL.Input.GamePad.2",
      "SDL.Input.GamePad.3"
    ],
    "controls": {
      "A": {
        "keys": ["SDL.Input.GamePad.0A"]
      },
      "B": {
        "keys": ["SDL.Input.GamePad.0B"]
      },
      "Axis Y-": {
        "keys": ["SDL.Input.GamePad.0Up"]
      },
      "Axis Y+": {
        "keys": ["SDL.Input.GamePad.0Down"]
      },
      "Axis X-": {
        "keys": ["SDL.Input.GamePad.0Left"]
      },
      "Axis X+": {
        "keys": ["SDL.Input.GamePad.0Right"]
      },
      "Start": {
        "keys": ["SDL.Input.GamePad.0Start"]
      },
      "Select": {
        "keys": ["SDL.Input.GamePad.0Select"]
      }
    },
    "controlFormat": {
      "axis": "$CONTROL",
      "button": "$CONTROL",
      "key": "$CONTROL"
    }
  }
}
