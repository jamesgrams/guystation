{
  "command": "desmume",
  "saveDirFlag": "--save-dir",
  "screenshotsDirFlag": "--screenshot-dir",
  "saveDirArgType": " ",
  "screenshotsDirArgType": " ",
  "extraFlags": [],
  "fullScreenButtons": ["f11"],
  "activateCommand": "wmctrl -a 'DeSmuME'",
  "config": {
    "file": "/home/james/.config/desmume/config",
    "controls": {
      "A": {
        "keys": ["JOYKEYS","A"],
        "keyboardKeys": ["KEYS","A"]
      },
      "B": {
        "keys": ["JOYKEYS","B"],
        "keyboardKeys": ["KEYS","B"]
      },
      "X": {
        "keys": ["JOYKEYS","X"],
        "keyboardKeys": ["KEYS","X"]
      },
      "Y": {
        "keys": ["JOYKEYS","Y"],
        "keyboardKeys": ["KEYS","Y"]
      },
      "R": {
        "keys": ["JOYKEYS","Boost"],
        "keyboardKeys": ["KEYS","Boost"]
      },
      "L2": {
        "keys": ["JOYKEYS","L"],
        "keyboardKeys": ["KEYS","L"]
      },
      "R2": {
        "keys": ["JOYKEYS","R"],
        "keyboardKeys": ["KEYS","R"]
      },
      "Axis Y-": {
        "keys": ["JOYKEYS","Up"],
        "keyboardKeys": ["KEYS","Up"]
      },
      "Axis Y+": {
        "keys": ["JOYKEYS","Down"],
        "keyboardKeys": ["KEYS","Down"]
      },
      "Axis X-": {
        "keys": ["JOYKEYS","Left"],
        "keyboardKeys": ["KEYS","Left"]
      },
      "Axis X+": {
        "keys": ["JOYKEYS","Right"],
        "keyboardKeys": ["KEYS","Right"]
      },
      "Start": {
        "keys": ["JOYKEYS","Start"],
        "keyboardKeys": ["KEYS","Start"]
      },
      "Select": {
        "keys": ["JOYKEYS","Select"],
        "keyboardKeys": ["KEYS","Select"]
      }
    },
    "controlFormat": {
      "axis": "$CONTROL",
      "button": "$CONTROL",
      "key": "$CONTROL"
    }
  }
}
