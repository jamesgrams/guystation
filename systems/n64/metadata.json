{
  "command": "mupen64plus",
  "frontendCommand": "m64py",
  "saveDirFlag": "--savedir",
  "screenshotsDirFlag": "--sshotdir",
  "saveDirArgType": " ",
  "screenshotsDirArgType": " ",
  "extraFlags": ["--fullscreen"],
  "activateCommand": "wmctrl -l | grep -E \"Glide64|Mupen64Plus\" | cut -d' ' -f1 | xargs wmctrl -i -a",
  "frontendActivateCommand": "wmctrl -a 'M64Py'",
  "argsFirst": true,
  "fullScreenFailsafe": "wmctrl -i -r $(wmctrl -l | grep -E \"Glide64|Mupen64Plus\" | cut -d ' ' -f 1) -b add,fullscreen",
  "getResolutionCommand": "wmctrl -lG | grep -E \"Glide64|Mupen64Plus\" | tr -s ' ' | cut -f 5,6 -d ' ' | tr ' ' x",
  "failsafeStateCheck": "xprop -id $(wmctrl -l | grep -E \"Glide64|Mupen64Plus\" | cut -d ' ' -f 1) | grep _NET_WM_STATE"
}
